<script>
  $(document).ready(function() {
    $(window).on('resize', function() {
      lineTop = ($('#timeline-644 .node').first().outerHeight(true) -100)+ 'px';
      lineHeight = ($('.node').last().offset().top - $('.node').first().outerHeight()) - ($('.node').first().offset().top + $('.node').first().outerHeight(true) - 150) + 'px';
      $("#timeline-644 .line").css({
        'top': lineTop,
        'height': lineHeight
      });
    }).trigger('resize');    
  });
</script>
