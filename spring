    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.matchHeight/0.7.2/jquery.matchHeight.js"></script>
    <script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
    
    <script>

      function carouselNormalization(endorsement) {
        var items = $(endorsement).find('.item .endorsement'), //grab all slides
            heights = [], //create empty array to store height values
            tallest; //create variable to make note of the tallest slide
        console.log(items);
        if (items.length) {
          function normalizeHeights() {
            items.each(function() { //add heights to array
              heights.push($(this).height()); 
            });
            tallest = Math.max.apply(null, heights); //cache largest value
            items.each(function() {
              $(this).css('min-height',tallest + 'px');
            });
          };
          normalizeHeights();

          $(window).on('resize orientationchange', function () {
            tallest = 0, heights.length = 0; //reset vars
            items.each(function() {
              $(this).css('min-height','0'); //reset min-height
            }); 
            normalizeHeights(); //run it again 
          });
        }
      }

      $(document).ready(function() {
        $('.video-modal').on("click", function() {
          var $modalId = $('#spring-modal');
          var $iframe = "<div class='embed-responsive embed-responsive-16by9'>" + "<iframe class='embed-responsive-item' src='" + $(this).data("src") + "?autoplay=1&enablejsapi=1modestbranding=1' frameborder='0' webkitallowfullscreen mozallowfullscreen allowfullscreen allow='autoplay; fullscreen'></iframe>" + "</div>";
          if(($(this).data('src')).includes('www.dropbox.com')) {
            $iframe = '<div class="embed-responsive embed-responsive-16by9"><video width="100%" height="" autoplay controls><source src="' + $(this).data('src') + '" type="video/mp4" /></video></div>';
          }
          $modalId.modal('show')
          $($modalId).on('shown.bs.modal', function(e) {
            $(this).find(".modal-body").append($iframe);
          })
          $($modalId).on('hide.bs.modal', function(e) {
            $(this).find(".modal-body").empty();
            $iframe = null;
          })
        });
        var endorsements = $('.endorsement-carousel');
        for(i=0;i<endorsements.length;i++) {
          carouselNormalization(endorsements[i])
        }

        // subnavigation (1) prevent default actions and (2) open collasped field below
        $("a[href^='#subnavigation-']").on('click', function(e) {
          e.preventDefault();
          var hash = $(this.hash);
          $('html, body').animate({
            scrollTop: ($(hash).offset().top - 50)
          }, 1000, function() {
            if($(hash).next().find('.collapsible')) {
              $(hash).next().find('.collapsible .collapsed').trigger('click');
            }
          });
        });

      });

    </script>

    <script>
      function resetSpringProfiles() {
        $('.spring-profiles__card').each(function() { $(this).removeClass('active') });
        $('.spring-profiles, .spring-profile').removeClass('active');
      }

      function activateCard($current) {
        if($(window).width() > 768) {
          $bio = null;
          nextElements = $current.nextAll();
          for(i=0;i<nextElements.length;i++) { 
            if($(nextElements[i]).hasClass('spring-profiles__bio-area') && $bio == null) {
              $bio = $(nextElements[i]);
            }
          }
          if(!$bio.hasClass('active')) {
            $('.spring-profiles__bio-area').each(function() { 
              $(this).slideUp({
                start:function() {
                  $(this).removeClass('active')
                }
              })
            });
          }

          $bio.find('.spring-profiles__bio-area__writeable').first().html($($current.data('bio-target')).html()).promise().done(function(){
            if(!$bio.hasClass('active')) {
              $bio.slideDown({
                start: function () {
                  $(this).css({
                    display: "flex"
                  });
                  $(this).addClass('active')
                }
              });
            }
            $('.spring-profiles').addClass('active');
            $current.addClass('active');
          });
        } else {
          var $modalId = $('#spring-modal')
          var $modalBody = $modalId.find(".modal-body");
          $modalBody.empty();
          $modalBody. append($($current.data('bio-target')).html());
          $modalId.modal('show');
        }
      }

      $(document).ready(function() {
        $('.spring-profiles__card').on('click', function() {
          resetSpringProfiles();
          $current = $(this);
          activateCard($current);
        });
        $('.spring-profiles__bio-area__close').on('click',function(e) {
          e.preventDefault();
          resetSpringProfiles();
          $(this).parent().removeClass('active');
          $(this).parent().slideUp();
        });
      });

    </script>

    <script>
      $(window).on('scroll', function() {
        if(window.scrollY >= 500) {
          $('#navbar-scrolling').addClass('active');
        } else {
          $('#navbar-scrolling').removeClass('active');
        }
      }).trigger('scroll');
    </script>
    
    <script>
      $(document).ready(function() {
        $('.slideshow').each(function() {
          slideshow = $(this);
          slideshow.slick({
            infinite: true,
            slidesToShow: 3,
            slidesToScroll: 1,
            autoplay: true,
            autoplaySpeed: 2000,
            slide: '.slideshow__item',
            prevArrow: slideshow.find('.slideshow__prev'),
            nextArrow: slideshow.find('.slideshow__next'),
            responsive: [
              {
                breakpoint: 992,
                settings: {
                  slidesToShow: 2,
                  slidesToScroll: 1
                }
              },
              {
                breakpoint: 600,
                settings: {
                  slidesToShow: 1,
                  slidesToScroll: 1
                }
              }
						]
          });
        });
      });
    </script>

